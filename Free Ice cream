#include<bits/stdc++.h>
using namespace std ;
int main()

{

long long int n, x,j ;
long long int count=0 ;
cin >> n >> x ;

for(int i =0 ;i<n ; i++)
    {
    char c ;
    cin >> c >> j ;

    if(c== '+')
        x += j ;

    else if(j>x)
        count += 1 ;
    else
        x -= j ;
    }

cout << x << " "<< count << endl ;
return 0 ;
}
